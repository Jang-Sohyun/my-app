  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "dev:staging": "cross-env APP_ENV=DEV next dev",
    "dev:prod": "cross-env APP_ENV=PROD next dev",
    "dev": "npm run dev:staging",
    "build": "cross-env NODE_ENV=production next build",
    "postbuild": "next-sitemap",
    "start": "cross-env NODE_ENV=production next start",
    "export": "cross-env NODE_ENV=production next export",
    "test": "tsc --project tsconfig.json --noEmit",
    "deploy": "aws s3 sync ./out s3://ddle-web-prod --delete --profile=ddle && npm run invalidate-cache",
    "invalidate-cache": "aws cloudfront create-invalidation --distribution-id E3TIDSJPNU18L1 --paths /* --profile=ddle",
    "expo start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web"
  },
